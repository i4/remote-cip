DirectoryIndex connect.html

# Forward Websocket from for Xpra
RewriteEngine On
RewriteCond %{HTTP:Upgrade} websocket [NC]
RewriteRule (bello[0-7])([0-9][0-9])            ws://faui49$1.cs.fau.de:232$2/ [P,L]

# "Short"link for shared session
RewriteRule (bello[0-7][0-9]{2})([a-zA-Z0-9]{32}) /index.html?ssl=true&video=false&floating_menu=true&printing=false&keyboard_layout=de&reconnect=true&sharing=true&sound=false&steal=true&server=%{SERVER_NAME}&password=$2&path=/$1/ [L,R=301]
